<header class="navbar">
<div class="navbar-inner">
<div class="container">
<%= link_to(CONFIG['site_name'], root_path, class: "brand") %>
<ul class="nav pull-right">
<% CONFIG['header_links'].each do |link| %>
  <li class="divider-vertical"></li>
  <li><%= link_to(link.text, add_trailing_slash(link.path)) %></li>
  <% if link.resource.type == 'notebook' %><li>
    <%= link_to(image_tag('feed.png'), File.join(link.path, 'feed.atom'), target: '_blank') %>
  </li><% end %>
<% end %>
</ul>
</div>
</div>
</header>
<br/>